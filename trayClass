class Tray:
    def __init__(self,length,width):
        self.length = length #getLength()
        self.width = width   #getWidth()
        numOfBlks = 3
        blkArray = []
        '''
    def getLength(self):
        return length
    def getWidth(self):
        return width
        
    def getNumOfBlks(self):
        return numOfBlks

    def __str__(self):
        return "Hey i am a " + self.length + "x" + self.width + " tray object"
    '''
tray = Tray(2,2)
print(tray)
